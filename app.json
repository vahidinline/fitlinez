{
  "expo": {
    "name": "fitlinez",
    "slug": "fitlinez",
    "version": "40.0.9",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.fitlinez.eu",
      "buildNumber": "0.0.1",

      "supportsTablet": true,

      "infoPlist": {
        "NSHealthShareUsageDescription": "This app needs access to your step count data to help you track your fitness progress and set personalized activity goals.",
        "NSHealthUpdateUsageDescription": "This app requires access to write health data, such as weight, to provide personalized insights.",
        "NSHealthClinicalHealthRecordsShareUsageDescription": "This app needs access to your health data to provide better health insights."
      }
    },
    "android": {
      "versionCode": 57,
      "icon": "./assets/logo.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#5B5891"
      },
      "package": "com.fitlinez.eu",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "supportsRTL": true,
      "eas": {
        "projectId": "7819d490-9761-46a4-93c9-1c969843df38"
      },
      "bugsnag": {
        "apiKey": "47b625050059f2f5da5eed9ac8a07d60"
      }
    },
    "owner": "fitlinez",
    "plugins": [
      "expo-localization",
      "@bugsnag/plugin-expo-eas-sourcemaps",
      "expo-updates",
      "react-native-health",
      [
        "expo-updates",
        {
          "username": "fitlinez"
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ],
    "updates": {
      "url": "https://u.expo.dev/7819d490-9761-46a4-93c9-1c969843df38"
    },
    "runtimeVersion": "exposdk:49.0.0"
  }
}
